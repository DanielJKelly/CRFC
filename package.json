    "@types/lodash": "^4.14.104",
    "@types/mysql": "^2.15.2",
    "axios": "^0.18.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "client-sessions": "^0.8.0",
