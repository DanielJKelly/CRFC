  },
  "homepage": "https://github.com/DanielJKelly/crfc#readme",
  "devDependencies": {
    "@types/morgan": "^1.7.35",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.10",
    "extract-text-webpack-plugin": "^4.0.0-alpha.0",
    "file-loader": "^1.1.11",
    "jest": "^22.4.2",
    "morgan": "^1.9.0",
